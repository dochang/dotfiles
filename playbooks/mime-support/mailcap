# video/* & audio/*

application/ogg; mpv --really-quiet --force-window -- '%s'; test=test -n "$DISPLAY"
application/x-ogg; mpv --really-quiet --force-window -- '%s'; test=test -n "$DISPLAY"
application/sdp; mpv --really-quiet --force-window -- '%s'; test=test -n "$DISPLAY"
