# image/*

image/gif; sxiv -f '%s'; test=test -n "$DISPLAY"
image/jpeg; sxiv -f '%s'; test=test -n "$DISPLAY"
image/png; sxiv -f '%s'; test=test -n "$DISPLAY"
image/tiff; sxiv -f '%s'; test=test -n "$DISPLAY"
image/x-ms-bmp; sxiv -f '%s'; test=test -n "$DISPLAY"
image/x-portable-anymap; sxiv -f '%s'; test=test -n "$DISPLAY"
image/x-portable-bitmap; sxiv -f '%s'; test=test -n "$DISPLAY"
image/x-portable-graymap; sxiv -f '%s'; test=test -n "$DISPLAY"
image/x-portable-pixmap; sxiv -f '%s'; test=test -n "$DISPLAY"
image/x-xpixmap; sxiv -f '%s'; test=test -n "$DISPLAY"
