keybinds {
    locked {
        unbind "Ctrl g"
        // https://github.com/zellij-org/zellij/issues/418#issuecomment-1537037950
        bind "Alt #" {
            SwitchToMode "Normal"
        }
    }
    shared_except "locked" {
        unbind "Ctrl q"
    }
    shared_except "tmux" "locked" {
        unbind "Ctrl b"
    }
}
simplified_ui true

